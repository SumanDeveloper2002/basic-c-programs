#include<stdio.h>
int main()
{	
	int num1,num2;
	int result;
	char op;
	
	printf("Enter the numbers::");
	scanf("%d %d",&num1,&num2);
	
	printf("Enter operation::");
	scanf(" %c",&op);
	
	switch (op)
	{
		case '+':
			result=num1+num2;
			printf("%d",result);
			break;
		case '-':
			result=num1+num2;
			printf("%d",result);
			break;
		case '*':
			result=num1*num2;
			printf("%d",result);
			break;
		case '/':
			result=num1+num2;
			printf("%d",result);
			break;
		case'%':
			result=num1%num2;
			printf("%d",result);
			break;
		default:
			printf("Invalid operation, can't perform!");
			
	}
	return 0;
}

